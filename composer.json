{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.1.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "api-platform/core": "^3.3",
        "composer/package-versions-deprecated": "^1.11",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^4.0",
        "edumedia/tag-bundle": "^0.1.18",
        "handcraftedinthealps/goodby-csv": "^1.4",
        "insitaction/easyadmin-fields-bundle": "^5.1",
        "knplabs/knp-markdown-bundle": "1.10.0",
        "knplabs/knp-time-bundle": "^1.11",
        "lcobucci/jwt": "^5.4",
        "nelmio/cors-bundle": "^2.5",
        "nesbot/carbon": "^3.8",
        "phpdocumentor/reflection-docblock": "^5.4",
        "phpstan/phpdoc-parser": "^1.30",
        "sensio/framework-extra-bundle": "^6.0",
        "spomky-labs/web-push-bundle": "^3.1",
        "stof/doctrine-extensions-bundle": "^1.12",
        "symfony-bundles/json-request-bundle": "^4.1",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/finder": "6.4.*",
        "symfony/flex": "^2.0.0",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/http-foundation": "6.4.*",
        "symfony/http-kernel": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/stopwatch": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/ux-chartjs": "^2.0",
        "symfony/ux-react": "^2.21",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "6.4.*",
        "symfonycasts/micro-mapper": "^0.2.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.15",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "dbrekelmans/bdi": "^1.3",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.15",
        "symfony/panther": "^2.1",
        "symfony/var-dumper": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*",
        "zenstruck/foundry": "^1.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {},
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "server": {
            "serve": "symfony serve -d"
        },
        "yarn-install": {
            "install": "yarn install --force"
        },
        "watch": {
            "watch": "yarn watch"
        },
        "build": {
            "build": "yarn build"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
